{
  "name": "watchlyst",
  "version": "2.0.2",
  "author": {
    "name": "Araraura"
  },
  "description": "A Discord bot which allows server staff to create a list of troublesome users, and will alert when one of the users join the server.",
  "homepage": "https://github.com/Araraura/WatchLyst",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Araraura/WatchLyst.git"
  },
  "bugs": {
    "url": "https://github.com/Araraura/WatchLyst/issues"
  },
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "main": "build/src/main.js",
  "exports": "./build/main.js",
  "scripts": {
    "start": "node build/src/main.js",
    "dev": "ts-node-esm src/main.ts",
    "watch": "nodemon --exec ts-node-esm src/main.ts",
    "build": "tsc",
    "prebuild": "npm run clean",
    "clean": "rimraf build/"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "discord.js": "^14.7.1",
    "discordx": "^11.7.0",
    "dotenv": "^16.0.3",
    "pg": "^8.9.0",
    "sequelize": "^6.28.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=7.0.0"
  }
}
